{
  "name": "exampleviewer",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "ngsw-config": "node_modules/.bin/ngsw-config dist ./ngsw-config.json",
    "ngsw-copy": "copy node_modules/@angular/service-worker/ngsw-worker.js dist/ngsw-worker.js",
    "build-prod-ngsw": "node --max-old-space-size=8192 node_modules/@angular/cli/bin/ng build --prod",
    "serve-prod-ngsw": "npm run build-prod-ngsw && http-server dist -p 8080",
    "build:prod": "ng build --prod --aot",
    "analyze": "webpack-bundle-analyzer dist/stats.json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "json-server": "json-server --watch db.json --port 3004"
  },
  "window": {
    "show": true,
    "frame": true
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/schematics": "^9.0.2",
    "@angular/animations": "^9.0.1",
    "@angular/common": "^9.0.1",
    "@angular/compiler": "^9.0.1",
    "@angular/core": "^9.0.1",
    "@angular/forms": "^9.0.1",
    "@angular/http": "^7.2.16",
    "@angular/platform-browser": "^9.0.1",
    "@angular/platform-browser-dynamic": "^9.0.1",
    "@angular/router": "^9.0.1",
    "@angular/service-worker": "^9.0.1",
    "@rxweb/generics": "0.0.1-4",
    "@rxweb/http": "0.0.0-5",
    "@rxweb/reactive-dynamic-forms": "0.0.3",
    "@rxweb/reactive-form-validators": "^1.9.9-beta3",
    "@rxweb/sanitizers": "0.0.1-1",
    "@rxweb/storage": "0.0.1",
    "angular-instantsearch": "^2.1.0",
    "core-js": "^2.5.4",
    "highlight.js": "^9.17.1",
    "http-server": "^0.11.1",
    "json-server": "^0.15.1",
    "ngx-clipboard": "^12.3.1",
    "ngx-highlightjs": "^2.1.1",
    "rxjs": "^6.4.0",
    "rxjs-compat": "^6.3.3",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.2",
    "@angular/cli": "~9.0.2",
    "@angular/compiler-cli": "^9.0.1",
    "@angular/language-service": "^9.0.1",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "^5.4.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "^3.7.5",
    "webpack-bundle-analyzer": "^3.5.2"
  }
}
